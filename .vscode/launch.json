{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Web (Edge)",
      "type": "msedge",
      "request": "launch",
      "url": "http://localhost:5173",
      "webRoot": "${workspaceFolder}/src",
      "preLaunchTask": "start-web-dev-server",
      "userDataDir": "${workspaceFolder}/.vscode-edge-profile",
      "sourceMaps": true,
      "sourceMapPathOverrides": {
        "webpack:///./src/*": "${webRoot}/*"
      }
    }
  ],
  "compounds": []
}
